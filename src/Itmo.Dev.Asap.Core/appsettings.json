{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "Presentation": {
    "Kafka": {
      "Producers": {
        "AssignmentCreated": {
          "Topic": "asap_assignment"
        },
        "QueueUpdated": {
          "Topic": "asap_queue_updated"
        },
        "SubjectCourseCreated": {
          "Topic": "asap_subject_course"
        },
        "SubjectCoursePointsUpdated": {
          "Topic": "asap_subject_course_points"
        }
      }
    }
  }
}
